  <div class="container" ng-app="loginApp" ng-controller="loginCtrl">
		<h1>Login</h1>
      <form class="form-signin" action='login' method='post'>
        <label for="inputname" class="sr-only">Email address</label>
        <input type="text" id="inputname" ng-model='user.username' class="form-control" placeholder="Username" required="" autofocus="">
        <label for="inputPassword" class="sr-only">Password</label>
        <input type="password" id="inputPassword" ng-model='user.password' class="form-control" placeholder="Password" required="">
        <div class="checkbox">
          <label>
            <input type="checkbox" value="remember-me"> Remember me
          </label>
        </div>
        <button class="btn btn-lg btn-primary btn-block" type="button" ng-click="login()">Sign in</button>
      </form>

    </div>
	<script type="text/javascript" src="/components/angular/angular.min.js"></script>
	<script src="/js/modules/login/index.js">
	</script>